			if(r){
				var installmentIds = new Array();
			    $('.clsInstallmentforHostel:checked').each(function(){
			    	installmentIds.push($(this).attr("installmentId"));
			    });
			    if(eval(paymentAmount)>0 && paymentAmount!=undefined) {
